{% if mode|default and 'compact' == mode %}
    {% set compact = true %}
{% endif %}

<div class="row">
    <div class="col-lg-{% if compact|default %}12{% else %}4{% endif %}">
        <div class="card shadow mb-4">
            <div class="card-header py-3"à>
                <h6 class="m-0 font-weight-bold text-primary">
                    Paramètres du parcours de vie
                </h6>
            </div>

            <div class="card-body">
                <p>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.lifePathNumber }}</span>
                        <span class="text">Chemin de vie</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.idealNumber }}</span>
                        <span class="text">Nombre d'idéal</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.secretNumber }}</span>
                        <span class="text">Nombre secret</span>
                    </a>
                </p>

                <p>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.majorTurnNumber }} ans</span>
                        <span class="text">Tournant majeur</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.personalYearNowNumber }}</span>
                        <span class="text">Année personnelle {{ "now"|date('Y') }}</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.astrologicalNumber }}</span>
                        <span class="text">Nombre astrologique</span>
                    </a>
                </p>

                <p>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.veryShortTermNumber }}</span>
                        <span class="text">Nombre du très court terme</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.shortTermNumber }}</span>
                        <span class="text">Nombre du court terme</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.meanTermNumber }}</span>
                        <span class="text">Nombre du moyen terme</span>
                    </a>
                    <a href="#" class="btn btn-info btn-icon-split disabled-btn mb-2">
                        <span class="icon text-white-50">{{ subject.data.lifePath.longTermNumber }}</span>
                        <span class="text">Nombre du long terme</span>
                    </a>
                </p>
            </div>
        </div>
    </div>

    <div class="col-lg-{% if compact|default %}6{% else %}4{% endif %}">
        <div class="card shadow mb-4">
            <a href="#collapseCardExample0" class="d-block card-header py-3 collapsed" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="collapseCardExample0">
