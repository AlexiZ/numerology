            <div class="card">
                <div class="card-header" id="headingLifePath">
                    <h6 class="mb-0">
                        <a class="d-flex justify-content-between align-items-center" href="#" data-toggle="collapse" data-target="#collapseLifePath" aria-expanded="true" aria-controls="collapseLifePath">
                            Chemin de vie
                            <span class="badge badge-primary badge-pill">{{ subject.data.lifePath.lifePathNumber }}</span>
                        </a>
                    </h6>
                </div>

                <div id="collapseLifePath" class="collapse" aria-labelledby="headingLifePath" data-parent="#accordionLifePath">
                    <div class="card-body">
                        <p><small><em>{{ getDefinition('lifepath')|raw }}</em></small></p>
                        <p>{{ subject.data.lifePath.lifePathNumber|getAnalysis('lifepath')|raw }}</p>
                    </div>
                </div>
            </div>
        </div>

